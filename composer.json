{
    "name": "creocoder/yii2-flysystem",
    "description": "The flysystem extension for the Yii framework",
    "keywords": [
        "yii2",
        "flysystem",
        "filesystem",
        "files",
        "aws",
        "s3",
        "azure",
        "copy.com",
        "dropbox",
        "ftp",
        "gridfs",
        "rackspace",
        "sftp",
        "webdav"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexander Kochetov",
            "email": "creocoder@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "league/flysystem": "^2.0.0|^3.0.0"
    },
    "require-dev": {
        "league/flysystem-async-aws-s3": "^2.0.0|^3.0.0",
        "league/flysystem-aws-s3-v3": "^2.0.0|^3.0.0",
        "league/flysystem-azure-blob-storage": "^2.0.0|^3.0.0",
        "league/flysystem-ftp": "^2.0.0|^3.0.0",
        "league/flysystem-google-cloud-storage": "^2.0.0|^3.0.0",
        "league/flysystem-memory": "^2.0.0|^3.0.0",
        "league/flysystem-sftp-v3": "^2.0.0|^3.0.0",
        "league/flysystem-webdav": "^3.0.0",
        "league/flysystem-ziparchive": "^2.0.0|^3.0.0",
        "spatie/flysystem-dropbox": "^2.0.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "creocoder\\flysystem\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true
    }
}
